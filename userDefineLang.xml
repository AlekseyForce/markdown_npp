<NotepadPlus>
    <UserLang name="Markdown" ext="markdown mdown mkdn mdwn mkd md">
        <Settings>
            <Global caseIgnored="yes" escapeChar="\" />
            <TreatAsSymbol comment="no" commentLine="yes" />
            <Prefix words1="yes" words2="yes" words3="yes" words4="yes" />
        </Settings>
        <KeywordLists>
            <Keywords name="Delimiters">[_0]_0</Keywords>
            <Keywords name="Folder+"></Keywords>
            <Keywords name="Folder-"></Keywords>
            <Keywords name="Operators">+</Keywords>
            <Keywords name="Comment">1 2 0# 0## 0### 0#### 0#####</Keywords>
            <Keywords name="Words1">http:// (http:// https:// (https:// (/</Keywords>
            <Keywords name="Words2">** * -</Keywords>
            <Keywords name="Words3">====== -------</Keywords>
            <Keywords name="Words4">&gt;</Keywords>
        </KeywordLists>
        <Styles>
            <WordsStyle name="DEFAULT" styleID="11" fgColor="F8F8F8" bgColor="141414" fontName="" fontStyle="0" />
            <WordsStyle name="FOLDEROPEN" styleID="12" fgColor="F8F8F8" bgColor="141414" fontName="" fontStyle="0" />
            <WordsStyle name="FOLDERCLOSE" styleID="13" fgColor="F8F8F8" bgColor="141414" fontName="" fontStyle="0" />
            <WordsStyle name="KEYWORD1" styleID="5" fgColor="CDA869" bgColor="141414" fontName="" fontStyle="0" />
            <WordsStyle name="KEYWORD2" styleID="6" fgColor="8F9D6A" bgColor="141414" fontName="" fontStyle="1" />
            <WordsStyle name="KEYWORD3" styleID="7" fgColor="9B703F" bgColor="141414" fontName="" fontStyle="1" />
            <WordsStyle name="KEYWORD4" styleID="8" fgColor="CF6A4C" bgColor="141414" fontName="" fontStyle="0" />
            <WordsStyle name="COMMENT" styleID="1" fgColor="9B703F" bgColor="141414" fontName="" fontStyle="0" />
            <WordsStyle name="COMMENT LINE" styleID="2" fgColor="9B703F" bgColor="141414" fontName="" fontStyle="1" />
            <WordsStyle name="NUMBER" styleID="4" fgColor="F8F8F8" bgColor="141414" fontName="" fontStyle="0" />
            <WordsStyle name="OPERATOR" styleID="10" fgColor="8F9D6A" bgColor="141414" fontName="" fontStyle="1" />
            <WordsStyle name="DELIMINER1" styleID="14" fgColor="CDA869" bgColor="141414" fontName="" fontStyle="0" />
            <WordsStyle name="DELIMINER2" styleID="15" fgColor="8F9D6A" bgColor="141414" fontName="" fontStyle="1" />
            <WordsStyle name="DELIMINER3" styleID="16" fgColor="F8F8F8" bgColor="141414" fontName="" fontStyle="0" />
        </Styles>
    </UserLang>
</NotepadPlus>
